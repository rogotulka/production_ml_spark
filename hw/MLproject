name: multistep_example


entry_points:
    load_raw_data:
        parameters:
            proxy: srting
        command: "python load_raw_data.py --proxy {proxy}"
    etl_data:
        parameters:
          ratings_csv: path
          max_row_limit: {type:int, default: 100000}
        command: "python etl_data.py --rating_csv {rating_csv} --max-row-limit {max_row_limit}"